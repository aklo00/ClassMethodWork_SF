<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>EndDate_must_be_before_StartDate</fullName>
    <active>true</active>
    <errorConditionFormula>EndDate__c &lt;  StartDate__c</errorConditionFormula>
    <errorDisplayField>EndDate__c</errorDisplayField>
    <errorMessage>終了日は開始日より前に設定できません</errorMessage>
</ValidationRule>
